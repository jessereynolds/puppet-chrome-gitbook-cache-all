{
  "name": "Puppet GitBook Cache All",
  "description" : "Puppet GitBook Cache All Extension",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://puppet-kmo.gitbook.io/*"],
      "js": ["background.js"],
      "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
  
}